generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  // TODO: env("DATABASE_URL") に変更する
  url      = env("DATABASE_URL_DEV")
}

model Actor {
  id Int @id @default(autoincrement())
  name String
}

model WishList {
  id String @id @default(uuid())
  movieId String
  title String
  likes Int
}